<mapper namespace="user">
	<select id="login" parameterType="UserDTO" resultType="UserDTO">
		<![CDATA[
		select id, memberId, memberPW, memberName from member
		where memberId = #{memberId} and memberPW = #{memberPW}
		limit 0, 1
		]]>
	</select>

</mapper>